.userInfoRootView {
    border: 1rpx #ccc solid;
    border-radius: 15rpx;
    margin:30rpx 20rpx 10rpx 20rpx;
    height: 100px;
    display: flex;
    align-items: center;
}

.userInfoView {
    margin-left: 10rpx;
    height: 100%;
    font-size: 14x;
    margin-top: 60rpx;
    padding-left: 10rpx;
}

.authBtn {
    border: 2rpx #00F solid;
    border-radius: 30rpx;
    width: 200rpx;
    height: 40rpx;
}

.orderRootView {
    margin-top: 20rpx;
    padding-left: 20rpx;
    padding-right: 20rpx;
}

button::after {
    border: none;
}

.allOdersButton {
    /*wxss不能引用local图片，可以使用图片的base64编码或者在xml中使用全局属性style*/
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: 20px 20px;
    background-position: right center;
    background-color: transparent;
    margin-right: 0rpx;
    border: none;
    width: 90px;
    font-size: 14px;
}